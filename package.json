{
	"name": "notifymate",
	"version": "3.1.10",
	"type": "module",
	"main": "dist/index.js",
	"module": "dist/index.js",
	"types": "dist/index.d.ts",
	"files": [
		"dist"
	],
	"scripts": {
		"dev": "vite",
		"build": "vite build",
