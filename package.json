{
	"name": "notifymate",
	"version": "3.1.33",
	"type": "module",
	"sideEffects": false,
	"files": [
		"dist",
		"src/Notify.css"
	],
	"scripts": {
		"dev": "vite",
		"build": "vite build",
		"lint": "eslint .",
		"preview": "vite preview",
		"publish:pkg": "rimraf dist && npm run build && npm version patch && npm publish --access public"
	},
	"dependencies": {
		"vite-plugin-css-injected-by-js": "^3.5.2",
		"vite-plugin-dts": "^4.5.0"
	},
	"peerDependencies": {
		"react": "^18.0.0",
		"react-dom": "^18.0.0"
	},
	"devDependencies": {
		"@eslint/js": "^9.21.0",
		"@types/react": "^19.0.10",
		"@types/react-dom": "^19.0.4",
		"@vitejs/plugin-react": "^4.3.4",
		"eslint": "^9.21.0",
		"eslint-plugin-react-hooks": "^5.0.0",
		"eslint-plugin-react-refresh": "^0.4.19",
		"globals": "^15.15.0",
		"typescript": "^4.0.0",
		"typescript-eslint": "^8.24.1",
		"vite": "^6.2.0",
		"rimraf": "^5.0.0"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/raiyanu/react-notify.git"
	},
	"main": "./dist/notifymate.cjs.js",
	"module": "./dist/notifymate.es.js",
	"types": "./dist/index.d.ts",
	"exports": {
		".": {
			"import": "./dist/notifymate.es.js",
			"require": "./dist/notifymate.cjs.js",
			"types": "./dist/index.d.ts"
		}
	},
	"publishConfig": {
		"registry": "https://registry.npmjs.org/"
